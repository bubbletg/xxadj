.geren-view{
    display: block;
    width: 100%;
    height: 350rpx;
    background: #479DE6;
}
 .nav-list-geren {
    display:flex; 
    position: relative;
    color:#666;
    align-items:center;
    padding:20rpx;
    margin:40rpx;
    /* 相对定位 */
     margin-top:-300rpx; 
    border-radius:12rpx;
    box-shadow:0 2rpx 6rpx rgba(0, 0, 0, 0.1);
    background:#fff;

} 
.geren-view-image{
    border-radius:1000rpx;
    width:160rpx;
    height:160rpx;
    margin:20rpx;
    
}
.geren-view-name{
  margin:10rpx;
  font-size:40rpx;
  width:320rpx;
}
.cu-tag:empty:not([class*="cuIcon-"]) {
    padding:0rpx;
    width:16rpx;
    height:16rpx;
    top:40rpx;
    right:30rpx;
    
    }
    
